{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "DataBaseAccess/*.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**"
          ],
          "src": ".."
        }
      ],
      "dest": "api/DataBaseAccess"
    },
    {
      "src": [
        {
          "files": [
            "EmailLib/*.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**"
          ],
          "src": ".."
        }
      ],
      "dest": "api/EmailLib"
    },
    {
      "src": [
        {
          "files": [
            "WebApplication/*.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**"
          ],
          "src": ".."
        }
      ],
      "dest": "api/WebApplication"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "index.md",
          "toc.yml"
        ]
      },
      {
        "files": [
          "api/**/*.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**/*.yml",
          "articles/**/*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**/*.md"
        ]
      }
    ],
    "dest": "../docs",
    "globalMetadataFiles": [
      "globalMetadata.json"
    ],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "statictoc"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig"
  }
}